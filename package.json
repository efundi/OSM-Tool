{
  "name": "pdf-marker",
  "version": "5.0.0-beta.17",
  "description": "PDF Marker",
  "homepage": "https://github.com/efundi/PDF-Marker",
  "main": "main.js",
  "author": {
    "name": "OPENCOLLAB",
    "email": "sakai@opencollab.co.za"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/efundi/PDF-Marker.git"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "electron": "ng build --base-href ./ && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start": "npm-run-all -p ng:serve electron:serve",
    "debug": "npm-run-all -p ng:serve electron:debug",
    "build": "npm run build:dev",
    "build:dev": "npm run electron:webpack:dev && npm run ng:build:dev",
    "build:prod": "npm run electron:webpack:prod && npm run ng:build:prod",
    "deploy": "npm run build:prod && electron-builder build --win --publish always",
    "ng:build:prod": "ng build --configuration production",
    "ng:build:dev": "ng build",
    "ng:serve": "ng serve",
    "electron:webpack:dev": "webpack --config webpack.electron.config.js --progress",
    "electron:webpack:prod": "webpack --config webpack.electron.config.js --progress --env production=true",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:webpack:dev && electron . --inspect=5858 --serve",
    "electron:debug": "wait-on http-get://localhost:4200/ && npm run electron:webpack:dev && electron . --inspect-brk=5858 --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/cdk": "^17.1.1",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/material": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@material-design-icons/font": "^0.14.13",
    "@rxweb/reactive-form-validators": "^13.0.1",
    "annotpdf": "^1.0.15",
    "archiver": "^6.0.1",
    "bootstrap": "^5.3.2",
    "csvtojson": "^2.0.10",
    "electron-log": "^5.1.0",
    "electron-updater": "^6.1.7",
    "fs-extra": "^11.1.0",
    "glob": "^10.3.10",
    "json-2-csv": "^5.0.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "ngx-color-picker": "^16.0.0",
    "ngx-filesaver": "^17.0.0",
    "pdf-lib": "^1.4.0",
    "pdfjs-dist": "^2.15.349",
    "rxjs": "~7.8.0",
    "simple-excel-to-json": "^2.3.0",
    "tslib": "^2.3.1",
    "webpack-node-externals": "^3.0.0",
    "word-wrap": "^1.2.3",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@angular/language-service": "^17.1.1",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.54.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "~5.2.2",
    "wait-on": "^7.2.0",
    "webdriver-manager": "^12.1.7",
    "webpack-cli": "^5.1.4"
  }
}
