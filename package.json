{
  "name": "pdf-marker",
  "version": "4.0.0-beta.7",
  "description": "PDF Marker",
  "homepage": "https://github.com/efundi/PDF-Marker",
  "main": "main.js",
  "author": {
    "name": "OPENCOLLAB",
    "email": "sakai@opencollab.co.za"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/efundi/PDF-Marker.git"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "electron": "ng build --base-href ./ && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start": "npm-run-all -p ng:serve electron:serve",
    "debug": "npm-run-all -p ng:serve electron:debug",
    "build": "npm run electron:webpack && ng build",
    "build:dev": "npm run build --",
    "build:prod": "npm run build -- -c production",
    "deploy": "npm run build:prod && electron-builder build --win --publish always",
    "ng:build": "ng build --configuration production",
    "ng:serve": "ng serve",
    "ng:serve:web": "&& ng serve -o",
    "electron:webpack": "webpack --config webpack.electron.config.js --progress",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:webpack && electron . --inspect=5858 --serve",
    "electron:debug": "wait-on http-get://localhost:4200/ && npm run electron:webpack && electron . --inspect-brk=5858 --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@material-design-icons/font": "^0.11.8",
    "@rxweb/reactive-form-validators": "^2.1.6",
    "annotpdf": "^1.0.15",
    "bootstrap": "^5.2.0",
    "csvtojson": "^2.0.10",
    "electron-log": "^4.4.8",
    "electron-updater": "^4.6.5",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "json-2-csv": "^3.17.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "luxon": "^3.0.1",
    "ngx-color-picker": "^11.0.0",
    "ngx-filesaver": "^8.1.0",
    "pdf-lib": "^1.4.0",
    "pdfjs-dist": "^2.15.349",
    "rxjs": "~7.5.2",
    "simple-excel-to-json": "^2.3.0",
    "tslib": "^2.3.1",
    "webpack-node-externals": "^3.0.0",
    "word-wrap": "^1.2.3",
    "zip-dir": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^3.0.0",
    "@types/node": "^16.11.21",
    "codelyzer": "^6.0.2",
    "electron": "^20.0.0",
    "electron-builder": "^23.3.3",
    "electron-reload": "^2.0.0-alpha.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "~7.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslint": "^6.1.3",
    "typescript": "~4.7.4",
    "wait-on": "^3.3.0",
    "webdriver-manager": "^12.1.7",
    "webpack-cli": "^4.9.2"
  }
}
