{
  "name": "pdf-marker",
  "version": "3.0.0-beta.1",
  "description": "PDF Marker",
  "homepage": "https://github.com/efundi/PDF-Marker",
  "main": "main.js",
  "author": {
    "name": "OPENCOLLAB",
    "email": "sakai@opencollab.co.za"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/efundi/PDF-Marker.git"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "electron": "ng build --configuration electron --base-href ./ && electron .",
    "electron-tsc": "tsc main.ts && ng build --base-href ./ && electron .",
    "compile:server": "webpack --config webpack.server.config.js --progress",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --configuration production && ng run pdf-marker:server:production --bundle-dependencies all",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build --",
    "build:prod": "npm run build -- -c production",
    "buildElectron": "tsc main.ts && npm run build:ssr && electron-builder build --win --publish never",
    "deploy": "tsc main.ts && npm run build:ssr && electron-builder build --win --publish always",
    "ng:serve": "ng serve --proxy-config proxy.conf.json",
    "ng:serve:web": "&& ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && npm run build:ssr && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.3",
    "@angular/cdk": "~13.1.3",
    "@angular/common": "~13.1.3",
    "@angular/compiler": "~13.1.3",
    "@angular/core": "~13.1.3",
    "@angular/forms": "~13.1.3",
    "@angular/material": "^13.1.3",
    "@angular/platform-browser": "~13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.3",
    "@angular/platform-server": "~13.1.3",
    "@angular/router": "~13.1.3",
    "@nguniversal/express-engine": "^13.0.2",
    "@rxweb/reactive-form-validators": "^1.9.7",
    "annotpdf": "^1.0.15",
    "csvtojson": "2.0.0",
    "electron-log": "^4.4.5",
    "electron-updater": "^4.6.1",
    "etl": "^0.6.12",
    "express": "^4.15.2",
    "express-validator": "^6.2.0",
    "file-saver": "^2.0.2",
    "fs-extra": "^8.1.0",
    "hammerjs": "^2.0.8",
    "hex-rgb": "^4.1.0",
    "json-2-csv": "^3.5.7",
    "jszip": "^3.2.2",
    "locutus": "^2.0.11",
    "lodash-es": "^4.17.21",
    "multer": "^1.4.2",
    "ngx-color-picker": "^11.0.0",
    "ngx-filesaver": "^8.1.0",
    "pdf-lib": "^1.4.0",
    "pdfjs-dist": "^2.12.313",
    "rgb-hex": "^3.0.0",
    "rxjs": "~7.5.2",
    "simple-excel-to-json": "^2.2.1",
    "trash": "^7.2.0",
    "tslib": "^2.3.1",
    "unzipper": "^0.10.10",
    "word-wrap": "^1.2.3",
    "zip-dir": "^1.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@angular/language-service": "~13.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.11.21",
    "codelyzer": "^6.0.2",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.0",
    "protractor": "~7.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.3",
    "typescript": "~4.5.5",
    "wait-on": "^3.3.0",
    "webdriver-manager": "^12.1.7",
    "webpack-cli": "^4.9.2"
  }
}
